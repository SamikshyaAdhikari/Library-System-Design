<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.Label?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.AddCopyController">
   <children>
      <Pane prefHeight="254.0" prefWidth="321.0">
      
         <children>
            <Label layoutX="21.0" layoutY="68.0" text="Book ISBN" />
            <TextField fx:id="txtBookISBN" layoutX="120.0" layoutY="63.0" />
            <Button fx:id="btnSubmit" layoutX="95.0" layoutY="141.0" mnemonicParsing="false" onAction="#btnSubmitButtonClicked" text="Save" />
            <Button fx:id="btnCancel" layoutX="181.0" layoutY="141.0" mnemonicParsing="false" onAction="#btnExitButtonClicked" text="Cancel" />
            <Label fx:id="lblMessage" layoutX="21.0" layoutY="200.0" />
         </children>
      </Pane>
   </children>
</AnchorPane>
